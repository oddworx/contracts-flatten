{
  "scripts": {
    "chain": "hardhat node",
    "build": "hardhat compile",
    "setup": "ts-node scripts/setup.ts",
    "typechain": "yarn build && yarn typechain:genzee && yarn typechain:oddworx && yarn typechain:oddworx-staking && yarn typechain:golden-pass",
    "typechain:genzee": "typechain --target ethers-v5 --out-dir scripts/contracts artifacts/contracts/Genzee.sol/Genzee.json",
    "typechain:oddworx": "typechain --target ethers-v5 --out-dir scripts/contracts artifacts/contracts/Oddworx.sol/Oddworx.json",
    "typechain:oddworx-staking": "typechain --target ethers-v5 --out-dir scripts/contracts artifacts/contracts/OddworxStaking.sol/OddworxStaking.json",
    "typechain:golden-pass": "typechain --target ethers-v5 --out-dir scripts/contracts artifacts/contracts/GoldenPass.sol/GoldenPass.json"
  },
  "dependencies": {
    "ethers": "^5.6.2",
    "hardhat": "^2.9.2",
    "merkletreejs": "^0.2.31"
  },
  "devDependencies": {
    "@typechain/ethers-v5": "^10.0.0",
    "@types/node": "^17.0.23",
    "ts-node": "^10.7.0",
    "typechain": "^8.0.0",
    "typescript": "^4.6.3"
  }
}
